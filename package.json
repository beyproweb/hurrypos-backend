{
  "name": "hurrypos-backend",
  "version": "1.0.0",
  "description": "Backend for HurryPOS system",
  "main": "index.js",
  "scripts": {
    "build:win": "npx pkg local-print-bridge.js --targets node18-win-x64   --output dist/beypro-bridge-win-x64.exe",
    "build:mac": "npx pkg local-print-bridge.js --targets node18-macos-x64 --output dist/beypro-bridge-macos-x64",
    "build:linux": "npx pkg local-print-bridge.js --targets node18-linux-x64 --output dist/beypro-bridge-linux-x64",
    "build:all": "npm run build:win && npm run build:mac && npm run build:linux",
    "pack:win": "mkdir -p public/bridge && zip -j public/bridge/beypro-bridge-win-x64.zip dist/beypro-bridge-win-x64.exe installers/windows/install-beyprobridge.ps1 installers/windows/install-beyprobridge.cmd",
    "pack:mac": "mkdir -p public/bridge && (cd dist && zip -9 ../public/bridge/beypro-bridge-mac.zip beypro-bridge-macos-x64 ../installers/macos/install-beyprobridge.sh)",
    "pack:linux": "mkdir -p public/bridge && (cd dist && tar -czf ../public/bridge/beypro-bridge-linux-x64.tar.gz beypro-bridge-linux-x64 ../installers/linux/install-beyprobridge.sh)",
    "pack:all": "npm run pack:win && npm run pack:mac && npm run pack:linux"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cheerio": "^1.1.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "iyzipay": "^2.0.64",
    "json2csv": "^6.0.0-alpha.2",
    "moment-timezone": "^0.5.48",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "openai": "^4.98.0",
    "pdfkit": "^0.12.3",
    "pg": "^8.14.0",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0",
    "whatsapp-web.js": "^1.31.0",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "http-proxy-middleware": "^3.0.5",
    "nodemon": "^3.1.10"
  },
  "bin": "local-print-bridge.js"
}
